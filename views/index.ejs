<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-sRIl4kxILFvY47J16cr9ZwB07vP4J8+LH7qKQnuqkuIAvNWLzeN8tE5YBujZqJLB" crossorigin="anonymous">
<%- include('include/header') %>


<body>
    <h1>Quora POST</h1>
    <% for(let post of posts) { %>
    <div class="post">
        <h3 >@<%= post.username %></h3>
        <h3 >Content :- <%= post.content %></h3>
        <div class="side1">
        <a href="/posts/<%=post.id%>">See in detail</a>
        <a href="/posts/<%=post.id%>/edit">Edit</a>
        <form action="/posts/<%=post.id%>?_method=DELETE" method="post">
        <button  class="btn btn-outline-danger">Delete post</button>
        </form>
        </div>
    </div>
        <%}%>

    <br>
    <br>
    <div class="side">
   <button  class="btn btn-outline-danger"><a href="/posts/new">Create new post</a></button>
    <button  class="btn btn-outline-danger"><a href="/logout">Log out</a></button>

   </div>
    <!-- <script src="/js/main.js"></script> -->
</body>
</html>

